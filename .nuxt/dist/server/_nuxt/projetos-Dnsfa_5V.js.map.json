ient-shift {\r\n  0%, 100% { background-position: 0% 50%; }\r\n  50% { background-position: 100% 50%; }\r\n}\r\n\r\n.floating-elements {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n}\r\n\r\n.float-element {\r\n  position: absolute;\r\n  width: 4px;\r\n  height: 4px;\r\n  background: #f97316;\r\n  border-radius: 50%;\r\n  opacity: 0.6;\r\n}\r\n\r\n.float-element:nth-child(1) { top: 20%; left: 10%; }\r\n.float-element:nth-child(2) { top: 60%; left: 20%; }\r\n.float-element:nth-child(3) { top: 30%; left: 80%; }\r\n.float-element:nth-child(4) { top: 70%; left: 75%; }\r\n.float-element:nth-child(5) { top: 40%; left: 50%; }\r\n.float-element:nth-child(6) { top: 80%; left: 60%; }\r\n\r\n.container {\r\n  position: relative;\r\n  z-index: 2;\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n}\r\n\r\n.cta-content {\r\n  text-align: center;\r\n  max-width: 800px;\r\n  margin: 0 auto;\r\n}\r\n\r\n.cta-icon {\r\n  width: 80px;\r\n  height: 80px;\r\n  margin: 0 auto 2rem;\r\n  background: linear-gradient(135deg, #f97316, #EF4444);\r\n  border-radius: 50%;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  color: white;\r\n  box-shadow: 0 20px 40px rgba(239, 68, 68, 0.3);\r\n}\r\n\r\n.cta-icon svg {\r\n  width: 40px;\r\n  height: 40px;\r\n}\r\n\r\n.cta-title {\r\n  font-size: clamp(2.5rem, 5vw, 4rem);\r\n  font-weight: 900;\r\n  margin-bottom: 1.5rem;\r\n  font-family: 'Inter', sans-serif;\r\n  background: linear-gradient(135deg, #f97316, #EF4444);\r\n  -webkit-background-clip: text;\r\n  -webkit-text-fill-color: transparent;\r\n  background-clip: text;\r\n}\r\n\r\n.cta-subtitle {\r\n  font-size: clamp(1.1rem, 2vw, 1.5rem);\r\n  line-height: 1.6;\r\n  margin-bottom: 3rem;\r\n  opacity: 0.9;\r\n  max-width: 600px;\r\n  margin-left: auto;\r\n  margin-right: auto;\r\n}\r\n\r\n.cta-buttons {\r\n  display: flex;\r\n  gap: 1.5rem;\r\n  justify-content: center;\r\n  margin-bottom: 4rem;\r\n  flex-wrap: wrap;\r\n}\r\n\